{
    "dataset": "CIFAR10",
    "model": "efficientnet0",
    "config": { 
        "max_epoch": 10
    },
    "hyperparams": {
        "batch_size": {
            "type": "int",
            "value_type": "preordered",
            "default" : 64,
            "range": [32, 48, 64, 80, 96]              
        },
        "lr" : {
            "type": "float",
            "value_type": "continuous",
            "default": 1e-2,
            "range": [1e-4, 5e-2]
        },
        "weight_decay" : {
            "type": "float",
            "value_type": "continuous",
            "default": 1e-4,
            "range": [1e-6, 1e-4]
        },        
        "optimizer": {
            "type": "str",
            "value_type": "categorical",
            "default": "SGD", 
            "range": [
                "SGD",
                "Adadelta",
                "Adagrad",
                "Adam",                
                "AdamW",
                "ASGD"
            ]            
        }     

    }
}